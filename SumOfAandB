public class 'SumOfAandB',{
    public static void main(String args[]){
        int a = 10;
        int b = 20;
        //addition
        int sum = a+b;
        System.out.println(sum);
        //subtraction
        int sub = a-b;
        System.out.println(sub);
        // multiplication
        int mul = a*b;
        System.out.println(mul);
        // division
        int div = a/b;
        System.out.println(div);

    }
}